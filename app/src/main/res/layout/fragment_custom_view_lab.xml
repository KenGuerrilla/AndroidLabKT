<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".customViewLab.CustomViewLab">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.itl.kg.androidlabkt.customViewLab.titleContentTextView.TitleContentTextView
            android:id="@+id/customViewLab_textViewA"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="8dp"
            app:title_value="Test Title A"
            app:content_value="This is test content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@id/customViewLab_textViewB"/>

        <com.itl.kg.androidlabkt.customViewLab.titleContentTextView.TitleContentTextView
            android:id="@+id/customViewLab_textViewB"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="8dp"
            app:title_value="Test Title B"
            app:content_value="This is test content"
            app:layout_constraintTop_toBottomOf="@id/customViewLab_textViewA"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>



    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>